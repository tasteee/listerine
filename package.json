{
  "name": "listerine",
  "version": "1.0.0-beta.13",
  "description": "Lightweight, Mongo-inspired, TypeScript-first approach to managing / querying arrays of complex data using an intuitive, developer-focused API.",
  "author": "rokki <hannahroksanne@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "rainbow-logo.png"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc",
    "verify": "npm run typecheck && npm run test && npm run build",
    "prepublishOnly": "npm run build:readme && npm run verify",
    "build:readme": "npx @appnest/readme generate --config ./docs/blueprint.json --input ./docs/blueprint.md"
  },
  "devDependencies": {
    "@appnest/readme": "^1.2.7",
    "prettier": "^3.5.3",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.2"
  },
  "dependencies": {
    "just-safe-get": "^4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tasteee/listerine.git"
  },
  "bugs": {
    "url": "https://github.com/tasteee/listerine/issues"
  }
}
